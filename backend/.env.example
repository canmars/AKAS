# ==================================
# AKAS BACKEND ENVIRONMENT VARIABLES
# ==================================

# Sunucu Portu (Backend API'nin çalışacağı port)
PORT=5000

# Supabase Bağlantı Ayarları
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key

# JWT Secret Key (Authentication için güvenli bir random string kullanın)
# Üretmek için şu yapılır: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
JWT_SECRET=your_jwt_secret_key_here_use_a_strong_random_string